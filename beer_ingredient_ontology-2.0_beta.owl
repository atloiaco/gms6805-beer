    <!-- http://purl.obolibrary.org/obo/GMS6805-beer/beer_00001000 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GMS6805-beer/beer_00001000">
        <obo:IAO_0000115 xml:lang="en">A leavening agent containing yeast cells which is offered for sale and used to raise the dough in making bread, for fermenting beer, or for making whiskey.</obo:IAO_0000115>
        <obo:IAO_0000117>Erik Schmidt</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">WordNet 3.1</obo:IAO_0000119>
        <rdfs:label xml:lang="en">commercial yeast strain</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GMS6805-beer/beer_00001001 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GMS6805-beer/beer_00001001">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GMS6805-beer/beer_00001000"/>
        <obo:IAO_0000115 xml:lang="en">Liquid yeast used for producing beer with clean, crisp flavor characteristics with low fruitness and mild ester production.</obo:IAO_0000115>
        <obo:IAO_0000117>Erik Schmidt</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">wyeastlab.com</obo:IAO_0000119>
        <rdfs:label xml:lang="en">wyeast American ale 1056</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GMS6805-beer/beer_00002001 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GMS6805-beer/beer_00002001">
        <obo:IAO_0000115>The primary source of beer color and a major contributor to beer flavor, aroma, and body. Proteins in the grains give structure to beer foam and minerals deliver many of the nutrients essential to yeast growth.</obo:IAO_0000115>
        <obo:IAO_0000117>Alex Loiacono</obo:IAO_0000117>
        <obo:IAO_0000119>http://www.aperfectpint.net/Brewing%20Grains.pdf</obo:IAO_0000119>
        <rdfs:label xml:lang="en">brewing grains</rdfs:label>
    </owl:Class>
    


